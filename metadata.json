{
  "name": "unibet-go",
  "version": "0.2.0",
  "author": "unibet",
  "summary": "Puppet module to manage Thoughtworks Go server and agent",
  "license": "Apache-2.0",
  "source": "https://github.com/unibet/puppet-go.git",
  "project_page": "https://github.com/unibet/puppet-go",
  "issues_url": "https://github.com/unibet/puppet-go/issues",
  "operatingsystem_support": [

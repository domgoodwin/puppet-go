GO_SERVER_SSL_PORT=<%= @server_ssl_port %>
export GO_SERVER_SSL_PORT
SERVER_WORK_DIR=<%= @lib_directory %>
export SERVER_WORK_DIR
<%- if @server_mem != nil -%>
SERVER_MEM=<%= @server_mem %>
